    "pspdfkit": "^2024.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.23.1",
    "react-toastify": "^10.0.5",
    "serve": "^14.2.3",
